.section{
  padding: 0 44rpx;
  display: flex;
  justify-content: space-between;
  padding-top: 30rpx;
  background: #fff;
  padding-bottom: 10rpx;
}

.swiper{
  width: 100%;
  height: 100%;
}

.sectionicon{
  position: relative;
}

.imgtext {
  position: absolute;
  font-size:24rpx;
  font-family:Source Han Sans CN;
  font-weight:400;
  color:rgba(255,255,255,1);
  left: 54rpx;
  top: 90rpx;
}

.page{
  background: #f5f5f5;
}

.icontext{
  /* color: #63c2de; */
  font-weight: 700;
}

.iconimg{
  height: 134rpx;
  width: 203rpx;
}


.h2{
  font-size:29rpx;
  font-family:Adobe Heiti Std;
  font-weight:normal;
  color:rgba(51,51,51,1);
  margin-bottom: 10rpx;
  height: 30rpx;
  line-height: 30rpx;
}

.compontyitem{
  background: #fff;
  padding-top: 13rpx;
  padding-bottom: 13rpx;
  display: flex;
  border-bottom:2px solid rgba(238,238,238,1);
}

.componey{
  padding:22rpx;
  background: #fff;
}

.swiperwrap{
  height: 357rpx;
}

.compontyitem .img{
  width: 270rpx;
  height: 175rpx;
  margin-right: 21rpx;
}

.compontyitem .detail{
  flex: 1;
  height: 175rpx;
  overflow: auto;
}

.img .imgs{
  height: 100%;
  width: 100%;
}

.topic{
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
  font-size:27rpx;
  font-family:Source Han Sans CN;
  font-weight:400;
  color:rgba(51,51,51,1);
  margin-bottom: 10rpx;
}

.neirong{
  overflow: auto;
  /* font-size:26px; */
  font-family:Source Han Sans CN;
  font-weight:400;
  font-size:26rpx;
  color:rgba(83,82,82,1);
  opacity:0.8;
}

.kefu {
  position: relative;
}

.btn {
  position: absolute;
  top: 0;
}

.btn{ 
    width: 100%;
    height: 100%;
    background-color: transparent;   /*按钮透明transparent*/  
    border:none;				/*设置按钮边框*/
}

.btn::after{
  border: none;
}

.guanggao{
  height: 136rpx;
  padding: 0 22rpx 15rpx;
  background: #fff;
  margin-bottom: 5rpx;
}

.gimg{
  width: 100%;
  height: 100%;
}