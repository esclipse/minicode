.section{
  padding: 0 10rpx;
  display: flex;
  justify-content: space-around;
  background: #fff;
  padding-top: 30rpx;
}

.swiper{
  width: 100%;
  height: 100%;
}

.sectionicon{
  height: 200rpx;
  width: 600rpx;
}

.page{
  background: #f5f5f5;
}

.icontext{
  /* color: #63c2de; */
  font-weight: 700;
}

.iconimg{
  height: 100%;
  width: 100%;
}


.h2{
  font-size: 40rpx;
  display: flex;
  background: #fff;
  padding-left: 20rpx;
}

.compontyitem{
  height: 250rpx;
  background: #fff;
  display: flex;
  margin: 0 20rpx;
  border-bottom: 3rpx solid #b2b2b2;
  padding: 10rpx 5rpx;
}

.componey{
  background: #fff;
  padding-top: 20rpx;
}

.swiperwrap{
  height: 380rpx;
}

.compontyitem .img{
  width: 40%;
  height: 250rpx;
}

.compontyitem .detail{
  width: 60%;
  height: 250rpx;
  overflow: auto;
}

.img .imgs{
  height: 100%;
  width: 100%;
}

.topic{
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
  margin: 5rpx 0 5rpx 10rpx;
}

.neirong{
  color: #b2b2b2;
  font-size: 33rpx;
  margin: 0 0 10rpx 10rpx;
  overflow: auto;
}

.kefu {
  position: relative;
}

.btn {
  position: absolute;
  top: 0;
}

.btn{ 
    width: 100%;
    height: 100%;
    background-color: transparent;   /*按钮透明transparent*/  
    border:none;				/*设置按钮边框*/
}

.btn::after{
  border: none;
}

.guanggao{
  height: 150rpx;
  padding: 0 20rpx 20rpx;
  background: #fff;
  margin-bottom: 5rpx;
}

.gimg{
  width: 100%;
  height: 100%;
}